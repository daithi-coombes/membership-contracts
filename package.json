{
  "name": "hardhat-project",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.20.0",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.1",
    "@types/chai": "^4.2.6",
    "@types/console-log-level": "^1.4.0",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.2.0",
    "console-log-level": "^1.4.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-unused-imports": "^1.1.1",
    "ethereum-waffle": "^3.0.2",
    "ethers": "^5.0.8",
    "hardhat": "^2.2.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.11.15",
    "hardhat-gas-reporter": "^1.0.9",
    "lodash": "^4.17.21",
    "mocha-chai-jest-snapshot": "^1.1.0",
    "prettier": "^2.2.1",
    "prettier-check": "^2.0.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "prompts": "^2.4.2",
    "solhint": "^3.2.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^8.5.4",
    "typechain": "^5.0.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "compile": "hardhat clean && hardhat compile",
    "test": "hardhat test",
    "lint": "eslint . --ext .ts",
    "prettier:check": "prettier-check contracts/**/*.sol test/**/*.ts typechain/*.ts",
    "prettier": "prettier --write contracts/**/*.sol test/**/*.ts typechain/*.ts",
    "size-contracts": "hardhat clean && hardhat compile && hardhat size-contracts",
    "clear-cache": "rm -rf artifacts/ cache/ typechain/",
    "coverage": "RUN_COVERAGE=1 hardhat coverage",
    "deploy:minter:void": "hardhat run --network hardhat scripts/deploy/minter/minter.ts",
    "deploy:minter:goerli": "hardhat run --network goerli scripts/deploy/minter/minter.ts",
    "deploy:minter:gnosis": "hardhat run --network gnosis scripts/deploy/minter/minter.ts",
    "deploy:registry:void": "hardhat run --network hardhat scripts/deploy/registry/registry.ts",
    "deploy:registry:goerli": "hardhat run --network goerli scripts/deploy/registry/registry.ts",
    "deploy:registry:gnosis": "hardhat run --network gnosis scripts/deploy/registry/registry.ts"
  },
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.7.3"
  }
}